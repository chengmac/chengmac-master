{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.27",
    "antd": "^4.8.2",
    "braft-editor": "^2.3.9",
    "dva-loading": "^3.0.21",
    "js-cookie": "^2.2.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.0.7",
    "loadsh": "^0.0.4",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.27",
    "yorkie": "^2.0.0"
  }
}
